    "types": [ "@nuxt/kit", "@types/node", "nuxt"],
    "paths": {
      "#imports": ["./.nuxt/imports.d.ts"],
    },
    "moduleResolution": "node",
    "ignoreDeprecations": "6.0"
  },
  "vueCompilerOptions": {
    "globalTypesPath": ".nuxt/global.d.ts"
  }
}